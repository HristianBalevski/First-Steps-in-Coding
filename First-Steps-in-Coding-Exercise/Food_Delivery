chiken_cnt = int(input())
fish_cnt = int(input())
vegetarian_cnt = int(input())

chiken_menu = chiken_cnt * 10.35
fish_menu = fish_cnt * 12.40
vegetarian_menu = vegetarian_cnt * 8.15

total_sum = chiken_menu + fish_menu + vegetarian_menu
desert = total_sum * 0.20

delivery_price = total_sum + desert + 2.50

print(delivery_price)
